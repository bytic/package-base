{
  "name": "bytic/package-base",
  "description": "package-base for bytic library",
  "license": "MIT",
  "type": "library",
  "keywords": [],
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "homepage": "https://github.com/bytic/package-base",
  "require": {
    "bytic/container": "^1.0 || ^2.0"
  },
  "require-dev": {
    "bytic/orm": "^0.9 || ^1.0",
    "bytic/phpqatools": "^1.0",
    "mockery/mockery": "^1.1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "ByTIC\\PackageBase\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\PackageBase\\Tests\\": "tests/src",
      "ByTIC\\PackageBase\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "bytic/phpqatools": true
    }
  }
}
